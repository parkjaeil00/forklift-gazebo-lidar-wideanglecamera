        </clip>
        <lens>
          <type>custom</type>
          <custom_function>
            <c1>1.05</c1>
            <c2>4</c2>
            <f>1.0</f>
            <fun>tan</fun>
          </custom_function>
          <cutoff_angle>1.5707</cutoff_angle>   <!-- 원형 or 사각형-->
          <scale_to_hfov>true</scale_to_hfov>
          <env_texture_size>512</env_texture_size>
        </lens>
      </camera>
      <plugin name="fisheye_camera_controller_2" filename="libgazebo_ros_camera.so">
        <always_on>true</always_on>
        <update_rate>30.0</update_rate>
        <camera_name>fisheye_camera_2</camera_name>
        <image_topic_name>/fisheye_camera_2/image_raw</image_topic_name>
        <camera_info_topic_name>/fisheye_camera_2/camera_info</camera_info_topic_name>
        <frame_name>fisheye_link_2</frame_name>
        <robotNamespace>/fisheye_camera_2</robotNamespace>
      </plugin>

    </sensor>
  </gazebo>

