  <!-- plugin -->
  <!--rear-->
  <gazebo reference="fisheye_link">
    <sensor name="fisheye_sensor" type="wideanglecamera">
      <always_on>true</always_on>
      <update_rate>30</update_rate>
      <camera>
        <horizontal_fov>3.1415</horizontal_fov> <!-- 수평 시야각-->
        <image>
          <width>320</width>
          <height>240</height>
        </image>
        <clip>
          <near>0.1</near>
          <far>100</far>
        </clip>
        <lens>
          <type>custom</type>
          <custom_function>
            <c1>1.05</c1>
            <c2>4</c2>
            <f>1.0</f>
            <fun>tan</fun>
          </custom_function>
          <cutoff_angle>1.5707</cutoff_angle>
          <scale_to_hfov>true</scale_to_hfov>
          <env_texture_size>512</env_texture_size>
        </lens>
      </camera>
      <plugin name="fisheye_camera_controller" filename="libgazebo_ros_camera.so">
        <always_on>true</always_on>
        <update_rate>30.0</update_rate>
        <camera_name>fisheye_camera</camera_name>
        <image_topic_name>/fisheye_camera/image_raw</image_topic_name>
        <camera_info_topic_name>/fisheye_camera/camera_info</camera_info_topic_name>
        <frame_name>fisheye_link</frame_name>
        <robotNamespace>/fisheye_camera</robotNamespace>
      </plugin> 

    </sensor>
  </gazebo>

